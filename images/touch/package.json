{
  "name": "collagegent",
  "productName": "collagegent",
  "version": "1.0.0",
  "description": "Application for collage gent",
  "main": "src/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run build-watch\" \"npm run serve-live\"",
    "build-watch": "watchify ./src/main.js -o servable/bundle.js",
    "serve-live": "live-server ./servable --port=3000",
    "start": "browserify /usr/app/src/main.js -o /usr/app/servable/bundle.js && serve /usr/app/servable",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Jan Everaert",
    "email": "jan@crashlab.be"
  },
  "license": "MIT",
  "dependencies": {
    "browserify": "^17.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "live-server": "^1.2.2",
    "serve": "^14.0.1",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "watch-server": "^1.3.0",
    "watchify": "^4.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "concurrently": "^7.6.0",
    "electron": "23.1.1"
  }
}
